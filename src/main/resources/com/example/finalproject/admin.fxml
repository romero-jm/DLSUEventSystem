<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="531.0" prefWidth="588.0" spacing="15" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.finalproject.AdminController">
    <children>
        <Label style="-fx-font-size: 24px;" text="Administrator Panel" />
        <TableView fx:id="eventTable" prefWidth="500.0">
            <columns>
                <TableColumn fx:id="colRequester" text="Requester" />
                <TableColumn fx:id="colStatus" text="Status" />
                <TableColumn fx:id="colStart" text="Start Time" />
                <TableColumn fx:id="colEnd" text="End Time" />
                <TableColumn fx:id="colDate" text="Date" />
            </columns>
        </TableView>
        <Button fx:id="deleteButton" prefHeight="0.0" prefWidth="131.0" text="Delete Selected Event" />
    </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
